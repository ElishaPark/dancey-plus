<p id="notice"><%= notice %></p>

<h1>User Saved Videos</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Video</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @user_saved_videos.each do |user_saved_video| %>
      <tr>
        <td><%= user_saved_video.user %></td>
        <td><%= user_saved_video.video %></td>
        <td><%= link_to 'Show', user_saved_video %></td>
        <td><%= link_to 'Edit', edit_user_saved_video_path(user_saved_video) %></td>
        <td><%= link_to 'Destroy', user_saved_video, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New User Saved Video', new_user_saved_video_path %>
